{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: torchrun",
            "type": "debugpy",
            "request": "launch",
            "module": "torch.distributed.run",
            "console": "integratedTerminal",
            "cwd": "${cwd}",
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
            },
            "args": [
                "--nproc_per_node", "1",
                "--master_port", "2672",
                "main.py",
                "--wandb_mode", "disabled",
                "--base_architecture", "deit_tiny_patch16_224",
                "--data_set", "CUB2011U",
                "--data_path", "/workspace/Hyperbolic_Hierarchical_Protonet_dev/data",
                "--input_size", "224",
                "--output_dir", "../logs/CUB2011U/deit_tiny_patch16_224/1028-1e-4-adamw-0.05-200-protopformer",
                "--model", "deit_tiny_patch16_224",
                "--batch_size", "64",
                "--seed", "1028",
                "--opt", "adamw",
                "--sched", "cosine",
                "--warmup-epochs", "0",
                "--warmup-lr", "1e-4",
                "--decay-epochs", "10",
                "--decay-rate", "0.9",
                "--weight_decay", "0.05",
                "--epochs", "200",
                "--finetune", "protopformer",
                "--features_lr", "1e-4",
                "--add_on_layers_lr", "3e-3",
                "--prototype_vectors_lr", "3e-3",
                "--last_layer_lr", "1e-4",
                "--last_layer_global_lr", "1e-4",
                "--curv_lr", "5e-4",
                "--visual_alpha_lr", "5e-4",
                "--prototype_shape", "2000", "192", "1", "1",
                "--reserve_layers", "11",
                "--reserve_token_nums", "81",
                "--use_global", "True",
                "--use_ppc_loss", "False",
                "--ppc_cov_thresh", "1.",
                "--ppc_mean_thresh", "2.",
                "--global_coe", "0.5",
                "--global_proto_per_class", "10",
                "--ppc_cov_coe", "0.1",
                "--ppc_mean_coe", "0.5"
            ],
        }
    ]
}


